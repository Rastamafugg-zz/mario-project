{
  "name": "mario-project",
  "version": "1.0.0",
  "description": "A Javascript/Phaser project using Super Mario Bros 1 as a model for a complete game engine.",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development && webpack-dev-server --content-base build/ --port 8100",
    "build.dev": "set NODE_ENV=development && webpack --config webpack.config.js",
    "build.prod": "set NODE_ENV=production && webpack --config webpack.config.js -p"
